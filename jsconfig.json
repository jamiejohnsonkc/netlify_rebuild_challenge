{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "root/*": ["./*"],
      "customStyles/*": ["./*"],
      "svg/*": ["**/svg/*"]
    }
  },
  "exclude": ["node_modules"]
}
